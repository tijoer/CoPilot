{
  "image": "mcr.microsoft.com/devcontainers/universal:2",
  "customizations": {
    "codespaces": {
      "openFiles": [
        "src/App.js"
      ]
	},
	"vscode": {
		"extensions": [
			"GitHub.copilot"
		]
	}
  },
  "portsAttributes": {
	"3000": {
		"label": "Application",
		"onAutoForward": "openPreview"
	},
	"8080": {
		"label": "nginx"
	}
},
  "forwardPorts": [3000]
}
